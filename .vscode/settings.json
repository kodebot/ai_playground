{
    "saveAndRun": {
        "commands": [
            {
                "match": "\\.*",
                "isAsync": true,
                "cmd": "cd ${workspaceFolder} && ${workspaceFolder}/.auto/commit.sh",
            }
        ]
    },
    "files.autoSave": "off",
    "jupyter.notebookFileRoot": "${workspaceFolder}",
    "peacock.color": "#215732"
}